@article{ WOS:A1992KE42300002,
Author = {LEE, BG and FISHER, NS},
Title = {{DECOMPOSITION AND RELEASE OF ELEMENTS FROM ZOOPLANKTON DEBRIS}},
Journal = {{MARINE ECOLOGY PROGRESS SERIES}},
Year = {{1992}},
Volume = {{88}},
Number = {{2-3}},
Pages = {{117-128}},
Month = {{NOV}},
Abstract = {{In experiments examining the retention of trace elements in decomposing
   zooplankton debris, the rates of C degradation and metal release from
   radiolabeled copepod fecal pellets and carcasses were measured for up to
   1 mo using radiotracers. Fecal pellets incubated at 18 and 2-degrees-C
   retained 35 to 40 \% and 80 \% of their C-14, respectively, after 30 d.
   Carcasses retained only 13 to 18 \% and 28 to 38 \% of their C-14 after
   18 d at 18 and 2-degrees-C, respectively. Leaching of C-14 from fecal
   pellets and carcasses as (DOC)-C-14 accounted for about half of the C-14
   loss, even in the absence of microbial activity. Proportionately more of
   the C-14 from carcasses was microbially oxidized to (CO2)-C-14 than C-14
   from fecal pellets. Release of the particle-reactive transuranic element
   Am-241 from fecal pellets was unaffected by microbial activity while
   release of Se-75 and Zn-65 from both fecal pellets and carcasses
   increased with microbial activity and closely followed C-14 loss.
   Release rates of all elements decreased exponentially over time, with
   the most pronounced decreases occurring within the first 6 d. Retention
   half-times (t(r1/2)'s) of Am-241 were greater-than-or-equal-to 48 d in
   fecal pellets. By contrast, the t(r1/2)'s for Zn-65 ranged from 2.1 to
   13.5 d in fecal pellets and both Zn-65 and Se-75 had t(r1/2)'s of only
   about 1 d in carcasses. The results help explain oceanographic
   observations that C, Se and Zn are recycled in surface waters while
   scavenged elements like Am are enriched in fecal pellets and have short
   residence times in surface waters.}},
