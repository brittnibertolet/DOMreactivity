@article{ WOS:000276440000024,
Author = {Gihring, Thomas M. and Lavik, Gaute and Kuypers, Marcel M. M. and
   Kostka, Joel E.},
Title = {{Direct determination of nitrogen cycling rates and pathways in Arctic
   fjord sediments (Svalbard, Norway)}},
Journal = {{LIMNOLOGY AND OCEANOGRAPHY}},
Year = {{2010}},
Volume = {{55}},
Number = {{2}},
Pages = {{740-752}},
Month = {{MAR}},
Abstract = {{Results from a study of benthic nitrogen cycling in two Arctic fjords
   are presented. Intact sediment core incubations were used to quantify
   net fluxes of dissolved inorganic nitrogen, organic nitrogen, organic
   carbon, and oxygen across the sediment-water interface. Rates of gross
   denitrification, anammox, nitrification, dissimilatory nitrate reduction
   to ammonium, and N-2 fixation were quantified using core incubations and
   slurry experiments. Profiles of dissolved inorganic nitrogen in
   pore-water, and organic carbon and nitrogen contents in the solid phase,
   were also obtained. Net nitrogen losses as N-2, measured directly as
   changes in N-2 : Ar, ranged from 152 mu mol N m(-2) d(-1) to 453 mu mol
   N m(-2) d(-1). Gross denitrification, as determined using N-15-NO3-
   tracers, was 34-294 mu mol N m(-2) d(-1). Rates of anammox ranged from
   10 mu mol N m(-2) d(-1) to 26 mu mol N m(-2) d(-1) and contributed
   5-23\% of gross N-2 generation. Nitrification rates were as high as 833
   mu mol N m(-2) d(-1) and sediments were a substantial source of nitrate
   to the water column (169-393 mu mol N m(-2) d(-1) efflux). Uptake of
   ammonium (52-87 mu mol N m(-2) d(-1)), dissolved organic nitrogen
   (291-486 mu mol N m(-2) d(-1)), and dissolved organic carbon (1313-2504
   mu mol N m(-2) d(-1)) were observed. Nitrogen loss from the seafloor is
   shown to occur at rates comparable to temperate coastal environments and
   the sediments provide a substantial source of nitrate to the water
   column of Arctic fjords. In addition, anammox is confirmed as an
   important pathway in Arctic marine sediments, although N-2 production is
   dominated by denitrification.}},
