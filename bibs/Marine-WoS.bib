@article{ WOS:000187362200008,
Author = {Vadstein, O and Olsen, LM and Busch, A and Andersen, T and Reinertsen,
   HR},
Title = {{Is phosphorus limitation of planktonic heterotrophic bacteria and
   accumulation of degradable DOC a normal phenomenon in phosphorus-limited
   systems? A microcosm study}},
Journal = {{FEMS MICROBIOLOGY ECOLOGY}},
Year = {{2003}},
Volume = {{46}},
Number = {{3}},
Pages = {{307-316}},
Month = {{DEC 5}},
Note = {{8th Symposium on Aquatic Microbial Ecology (SAME-8), SICILY, ITALY, OCT
   25-29, 2002}},
Abstract = {{A dual isotope labelling technique was used to follow the distribution
   of carbon and phosphorus in plankton microcosms containing autotrophs
   (Tetraselmis sp.), heterotrophic bacteria and herbivores (Brachionus
   plicatilis) at eight different total-P concentrations. P:C ratios of
   algae, bacteria and dissolved matter, as well as the general
   accumulation of degradable dissolved organic carbon, indicated that both
   the autotrophs and heterotrophic bacteria were P-limited in all
   microcosms. According to the theory, such coexistence should only be
   possible if bacteria have higher predation losses than algae, which was
   definitely not the case in our experiment. However, data are consistent
   with the assumption that bacteria are superior in P uptake but have a
   poor ability to retain acquired P, which would promote coexistence in a
   patchy P-supply environment resulting from nutrient regeneration by
   metazoan grazers. (C) 2003 Federation of European Microbiological
   Societies. Published by Elsevier B.V. All rights reserved.}},
