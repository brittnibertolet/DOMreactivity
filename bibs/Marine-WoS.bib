@article{ WOS:A1996VZ47600019,
Author = {Fry, B and Hopkinson, CS and Nolin, A},
Title = {{Long-term decomposition of DOC from experimental diatom blooms}},
Journal = {{LIMNOLOGY AND OCEANOGRAPHY}},
Year = {{1996}},
Volume = {{41}},
Number = {{6}},
Pages = {{1344-1347}},
Month = {{SEP}},
Abstract = {{Decomposition of C-13-labeled dissolved organic carbon (DOC) produced in
   two marine diatom blooms was followed for 2.5 yr with large volume (20
   liter) incubations performed in the dark. The C-13 tracer was used to
   partition decomposition dynamics of the fresh diatom-derived DOC and the
   turnover of background DOC from Woods Hole Harbor. DOC from Woods Hole
   Harbor proved largely refractory, with DOC concentrations falling from
   122 to similar to 100 mu M C in 2.5 yr. DOC from the diatom blooms was
   more labile, but was also incompletely mineralized, with 25-35\%
   remaining after 2.5 yr. Neither nutrients nor labile carbon (dextrose)
   added at 1.5 yr significantly stimulated DOC mineralization. The
   experiments indicate that DOC produced in short-term blooms can be
   surprisingly resistant to microbial attack.}},
