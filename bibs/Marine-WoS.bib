@article{ WOS:000387684400039,
Author = {Ksionzek, Kerstin B. and Lechtenfeld, Oliver J. and McCallister, S.
   Leigh and Schmitt-Kopplin, Philippe and Geuer, Jana K. and Geibert,
   Walter and Koch, Boris P.},
Title = {{Dissolved organic sulfur in the ocean: Biogeochemistry of a petagram
   inventory}},
Journal = {{SCIENCE}},
Year = {{2016}},
Volume = {{354}},
Number = {{6311}},
Pages = {{456-459}},
Month = {{OCT 28}},
Abstract = {{Although sulfur is an essential element for marine primary production
   and critical for climate processes, little is known about the oceanic
   pool of nonvolatile dissolved organic sulfur (DOS). We present a
   basin-scale distribution of solid-phase extractable DOS in the East
   Atlantic Ocean and the Atlantic sector of the Southern Ocean. Although
   molar DOS versus dissolved organic nitrogen (DON) ratios of 0.11 +/-
   0.024 in Atlantic surface water resembled phytoplankton stoichiometry
   (sulfur/nitrogen similar to 0.08), increasing dissolved organic carbon
   (DOC) versus DOS ratios and decreasing methionine-S yield demonstrated
   selective DOS removal and active involvement in marine biogeochemical
   cycles. Based on stoichiometric estimates, the minimum global inventory
   of marine DOS is 6.7 petagrams of sulfur, exceeding all other marine
   organic sulfur reservoirs by an order of magnitude.}},
